   "main": "app.js",
   "scripts": {
      "build": "tsoa spec-and-routes && tsc",
      "dev": "tsoa swagger-and-routes && concurrently --kill-others \"npm run tsoa:watch\" \"ts-node-dev --respawn --pretty --transpile-only src/app.ts\"",
      "worker:dev": "ts-node-dev --respawn --pretty --transpile-only src/worker.ts",
      "start": "ts-node src/app.ts",
      "worker:start": "ts-node src/worker.ts",
      "tsoa": "tsoa swagger-and-routes",
      "tsoa:watch": "run-when-changed --watch \"./src/api/controllers/*.ts\" --exec \"tsoa swagger-and-routes\"",
      "typeorm": "typeorm-ts-node-commonjs",
      "prettier-format": "prettier --config .prettierrc src/**/*.ts --write",
      "test": "jest"
   },
